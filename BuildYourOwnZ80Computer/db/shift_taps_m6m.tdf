FUNCTION altsyncram_7g81 (address_a[7..0], address_b[7..0], clock0, clocken0, data_a[0..0], wren_a)
RETURNS ( q_b[0..0]);
FUNCTION cntr_arf (clk_en, clock)
RETURNS ( q[7..0]);

--synthesis_resources = lut 8 M9K 1 reg 8 
SUBDESIGN shift_taps_m6m
( 
	clock	:	input;
	shiftin[0..0]	:	input;
	shiftout[0..0]	:	output;
	taps[0..0]	:	output;
) 
VARIABLE 
	altsyncram2 : altsyncram_7g81;
	cntr1 : cntr_arf;
	clken	: NODE;

BEGIN 
	altsyncram2.address_a[] = cntr1.q[];
	altsyncram2.address_b[] = cntr1.q[];
	altsyncram2.clock0 = clock;
	altsyncram2.clocken0 = clken;
	altsyncram2.data_a[] = ( shiftin[]);
	altsyncram2.wren_a = B"1";
	cntr1.clk_en = clken;
	cntr1.clock = clock;
	clken = VCC;
	shiftout[0..0] = altsyncram2.q_b[0..0];
	taps[] = altsyncram2.q_b[];
END;
